        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MinecraftDownloader"
        mc:Ignorable="d"
        Title="Minecraft Downloader" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="MCVersionDataGrid" Margin="130,10,130,197" SelectionMode="Single"/>
        <Button Content="Download" FontSize="14" Click="Button_Click" Margin="309,320,309,31"/>
        <ProgressBar x:Name="progress" Margin="0,264,0,140" />
    </Grid>
</Window>
